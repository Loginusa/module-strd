<!--EA590E85A78C48019A66BBD4403E08F3-->  <OBKMO_WIDGET_CLASS_ID><![CDATA[F91EE05268D446CDA3007B6C1764B383]]></OBKMO_WIDGET_CLASS_ID>
<!--EA590E85A78C48019A66BBD4403E08F3-->  <HQL><![CDATA[select q.documentNo as noQuotation, sum(r.orderedQuantity) as qty, sum(r.lineNetAmount) as total from Order q
join q.orderLineList r
where q.documentStatus='DR' AND q.salesTransaction=true AND q.transactionDocument.sOSubType LIKE 'OB'

group by q.documentNo]]></HQL>
<!--EA590E85A78C48019A66BBD4403E08F3-->  <TYPE><![CDATA[HQL]]></TYPE>
