<!--145BDCFEF5BA4E22B5D4EA9C6359C30A-->  <OBKMO_WIDGET_CLASS_ID><![CDATA[45E1A1F4EC7D492E965CFB93E6DD71F6]]></OBKMO_WIDGET_CLASS_ID>
<!--145BDCFEF5BA4E22B5D4EA9C6359C30A-->  <HQL><![CDATA[SELECT DISTINCT  e.id as productID , e.searchKey  as searchKey, e.name as finishgoods FROM Product AS e
LEFT JOIN e.productCategory 
, ProductBOM b
WHERE e.productCategory.searchKey = 'FG' AND 
      e.bOMVerified = 'N' 
     AND e.id = b.product 
     AND b.id is not null
     ORDER BY e.name ASC]]></HQL>
<!--145BDCFEF5BA4E22B5D4EA9C6359C30A-->  <TYPE><![CDATA[HQL]]></TYPE>
<!--145BDCFEF5BA4E22B5D4EA9C6359C30A--></OBCQL_WIDGET_QUERY>

